<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="">
    <main class="d-flex flex-nowrap">
        <%- include('../partials/icons.ejs'); %>
            <%- include('../partials/headerNew'); %>

                <div class="b-example-divider b-example-vr"></div>

                <div class="container px-4 center">
                    <table class="table text-center">
                        <thead class="table-light">
                            <td>User ID</td>
                            <td>User Login</td>
                        </thead>
                        <tbody class="table-group-divider">
                            <% users.forEach(function(user){ %>
                                <tr class="">
                                    <td><a href="admin/<%- user.id %>">
                                            <%- user.id %>
                                        </a></td>
                                    <td>
                                        <%- user.profile.login %>
                                    </td>
                                </tr>
                                <% }); %>
                        </tbody>
                    </table>

                    <div class="row row-cols-5">
                        <button onclick="gotoCreate()" href="/admin/create">Create New User</button>
                    </div>
                </div>

                <div id="user-detail-info"></div>
    </main>
</body>

</html>